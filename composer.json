{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "ext-fileinfo": "*",
        "barryvdh/laravel-dompdf": "*",
        "darkaonline/l5-swagger": "^9.0",
        "endroid/qr-code": "^5.0",
        "evilfreelancer/routeros-api-php": "^1.5",
        "inertiajs/inertia-laravel": "^2.0",
        "intervention/image": "^3.7",
        "ispgo/ckeditor": "@dev",
        "ispgo/mikrotik": "@dev",
        "ispgo/settings-manager": "@dev",
        "ispgo/siigo": "@dev",
        "ispgo/smartolt": "@dev",
        "ispgo/wiivo": "@dev",
        "laravel/framework": "^12.0",
        "laravel/nova": "^5.0",
        "laravel/passport": "^12.0",
        "laravel/tinker": "^2.9",
        "predis/predis": "^2.3",
        "sereny/nova-permissions": "@dev",
        "ispgo/nap-manager": "@dev"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.44",
        "mockery/mockery": "^1.6",
        "mrmarchone/laravel-auto-crud": "^1.6",
        "nunomaduro/collision": "^8.0",
        "phpunit/phpunit": "^11.0.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "hansschouten/laravel-pagebuilder"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "repositories": {
        "nova": {
            "type": "composer",
            "url": "https://nova.laravel.com"
        },
        "0": {
            "type": "path",
            "url": "./nova-components/SettingsManager"
        },
        "1": {
            "type": "path",
            "url": "./nova-components/Ckeditor"
        },
        "2": {
            "type": "path",
            "url": "./packages/ispgo/wiivo"
        },
        "3": {
            "type": "path",
            "url": "./nova-components/Mikrotik"
        },
        "4": {
            "type": "path",
            "url": "./nova-components/Smartolt"
        },
        "5": {
            "type": "vcs",
            "url": "https://github.com/yeiider/nova-permissions"
        },
        "6": {
            "type": "path",
            "url": "./packages/ispgo/siigo"
        },
        "7": {
            "type": "path",
            "url": "./nova-components/NapManager"
        }
    }
}
